
page{
	width: 100%;
	height: 100%;
	position: relative;
	color: #333;
    line-height: 1;
    background-color: #F2F2F2;
    font-size: 24rpx;
    font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
    -ms-text-size-adjust: 100%;
	-webkit-text-size-adjust: 100%;
	-webkit-tap-highlight-color: transparent;
}
.weui-main__color {
    color: #ff6c00!important;
}
.weui-flex {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
}
.weui-flex__item {
    font-size:22rpx;
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;   
    text-align: center;
}
.weui-flex__hd, .weui-flex__bd {
    display:block;
    text-align:center;
    color:#000000;
}
.weui-flex__bd {
    margin-top: 10rpx;
    font-size:22rpx;
}
.weui-flex__bd_active {
    display: inline-block;
    padding: 0 12rpx;
    background: #fff;
    border-radius: 10rpx;
}

.weui-cell {
    padding: 20rpx 20rpx;
    font-size:24rpx;
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
}
.weui-cell__bd {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
}
.weui-cell__ft {
    text-align: right;
}
.weui-cell__ft_in-access {
  position: relative;
}
.weui-cell__ft_in-access::after {
    content:"";
    display:inline-block;
    height:18rpx;
    width:18rpx;
    border-width:4rpx 4rpx 0 0;
    border-color:#c8c8cd;
    border-style:solid;
    -webkit-transform:matrix(.71,.71,-.71,.71,0,0);
    transform:matrix(.71,.71,-.71,.71,0,0);
    position:relative;
    top:-4rpx;
    position:absolute;
    top:50%;
    margin-top:-8rpx;
    right:4rpx;
}

.weui-grids {
    border-top: 1rpx solid #D9D9D9;
}
.weui-grid {
    position: relative;
    display: inline-block;
    padding: 30rpx 0;
    width: 50%;
    box-sizing: border-box;
    border-right: 1rpx solid #D9D9D9;
    border-bottom: 1rpx solid #D9D9D9;
}
.weui-grid__icon {
  display: block;
  width: 50rpx;
  height: 50rpx;
  margin: 0 auto;
}
.weui-grid__title, .weui-grid__label {
  display: block;
  text-align: center;
  color: #000;
  font-size: 22rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.weui-grid__label {
    margin-top: 20rpx;
}